import pandas as pd

std_data=pd.read_csv(r"C:\Users\Rajasree\Desktop\Workshop\IRIS.csv")
std_data.shape

std_data.head(10)

print("MEAN")
std_data.sepal_length.mean()
std_data.sepal_width.mean()
std_data.petal_length.mean()
std_data.petal_width.mean()

print("MEDIAN")
std_data.sepal_length.median()
std_data.sepal_width.median()
std_data.petal_length.median()
std_data.petal_width.median()

print("MODE")
std_data.species.mode()

print("VARIANCE")
std_data.sepal_length.var()
std_data.sepal_width.var()
std_data.petal_length.var()
std_data.petal_width.var()

print("STANDARD DEVIATION")
std_data.sepal_length.std()
std_data.sepal_width.std()
std_data.petal_length.std()
std_data.petal_width.std()

print("RANGE")
max(std_data.sepal_length)-min(std_data.sepal_length)
max(std_data.petal_width)-min(std_data.petal_width)
max(std_data.petal_width)-min(std_data.petal_width)

print("SKEWNESS")
std_data.sepal_length.skew()
std_data.sepal_width.skew()
std_data.petal_length.skew()
std_data.petal_width.skew()

print("KURTOSIS")
std_data.sepal_length.kurt()
std_data.sepal_width.kurt()
std_data.petal_length.kurt()
std_data.petal_width.kurt()


